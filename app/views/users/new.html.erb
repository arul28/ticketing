<h2>User Login</h2>

<%= form_with(url: userlogin_url, local: true) do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit "Login", class: 'btn btn-primary' %>
  </div>
<% end %>

